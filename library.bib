journal = {Applied Energy},
keywords = {Control system technologies,Defence industry,Electrical propulsion,Energy storage,Hybrid propulsion,Marine systems,Non-linear control systems,Power systems,Ship design,Shipbuilding industry},
pages = {30--54},
publisher = {The Authors},
title = {{Design and control of hybrid power and propulsion systems for smart ships: A review of developments}},
