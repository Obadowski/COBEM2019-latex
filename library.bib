@article{Coscas1995,
abstract = {The actor model is a model of concurrent computation which is$\backslash$npowerful enough to represent all the operations needed to exploit$\backslash$nparallel and distributed machines. In this paper, we propose to model$\backslash$nactor programs with Predicate/Transition nets. Predicate/Transition nets$\backslash$nare high-level Petri nets with data structures instead of tokens and$\backslash$npredicates associated with transitions. This formalism permits to$\backslash$nexpress the operational semantics of an actor program in its entirety: a$\backslash$ntranslation scheme from the actor language PRAL-M to the$\backslash$nPredicate/Transition nets language PTN is described. This formalism$\backslash$nwhich has both a textual and a graphical representation, is also useful$\backslash$nfor debugging and visualizing actor programs. Using this scheme, a$\backslash$nprototype has been implemented to translate actor programs into$\backslash$nPredicate/Transition nets and to debug them by simulating their$\backslash$nexecution in a step by step mode},
