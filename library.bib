@article{Letia2014,
abstract = {Hybrid control refers to applications having a$\backslash$ndiscrete event part and a continuous one that involves$\backslash$nthe interactions of different model types. Their$\backslash$ncontrol synthesis is difficult due to the fact that$\backslash$nthey contain models that belong to different$\backslash$napproaches. In the current study the Time Petri Nets$\backslash$n(TPNs) are used for discrete event controlled part$\backslash$nmodel. For the continuous plant model is used a$\backslash$ndiscrete time system (DTS) and the Fuzzy Logic Control$\backslash$n(FLC) for its control. A new proposed method links the$\backslash$nTPN models with FLC models. The formal Time Petri Net$\backslash$nbased Language (TPNL) (used for TPNs description) is$\backslash$nenhanced to comprise the FLC functions and thus the$\backslash$nentire hybrid control system can be described. The$\backslash$nhybrid control descriptions constructed with the$\backslash$nextended TPNL are transformed into trees that are$\backslash$nfurther processed by Genetic Programming for control$\backslash$nstructure synthesis. The controller's parameters are$\backslash$nimproved by Genetic Algorithm (GA). The evolutionary$\backslash$nindividuals are organised in species taking into$\backslash$naccount the isomorphic distance between them. The$\backslash$nspecies evolution implemented by GP is alternated by$\backslash$nits adaptation performed by GA. New methods are used$\backslash$nfor the bloat control leading to a higher speed of$\backslash$nsolution search.},
author = {Letia, Tiberiu S. and Kilyen, Ors},
